Algoritmo PROGRAMA
	Definir num, resul1, result2, pot Como Real;
	
	Escribir "Ingrese un numero: ";
	Leer num
	Escribir "Ingrese con que potencia desea elevarlo :";
	Leer pot;
	
	resul1<-(num)^pot;
	
	escribir "Su resultado es: ",resul1
	
	result2<-(num)^(1/2)
	
	Escribir "Su raíz cuadrada es: ",result2;
	
	
	
FinAlgoritmo
