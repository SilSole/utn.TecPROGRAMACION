Proceso semana
	Definir num Como Entero;
	
	Repetir
		Escribir "INGRESE un NUMEROS DEL 1 AL 7";
		Leer num;
		Segun num Hacer
			1:
				Escribir "LUNES, ";
			2:
				Escribir "MARTES, ";
			3:
				Escribir "MIÉRCOLES, ";
			4:
				Escribir "JUEVES, ";
			5:
				Escribir "VIERNES, ";
			6:
				Escribir "SÁBADO, ";
			7:
				Escribir "DOMINGO, ";
			De Otro Modo:
				Escribir "NO COINCIDE";
		FinSegun
	Hasta Que num>=7;
	
	
	
	
FinProceso
