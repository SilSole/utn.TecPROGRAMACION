Algoritmo BODA
	Escribir "INGRESE CUANTOS AÑOS DE CASADO TIENE: ";
	Leer num;
	Segun num Hacer
		10:
			Escribir "Bodas de Hojalata";
		20:
			Escribir "Bodas de Porcelana";
		30:
			Escribir "Bodas de Perlas";
		40:
			Escribir "Bodas de Rubí";
		50:
			Escribir "Bodas de Oro";
		60:
			Escribir "Bodas de Diamante";
		De Otro Modo:
			Escribir "NO TIENE ANIVERSARIO";
	FinSegun
	
FinAlgoritmo
